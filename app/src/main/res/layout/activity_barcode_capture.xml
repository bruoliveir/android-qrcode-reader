<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_barcode_capture"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.bruno.qrcodereader.BarcodeCaptureActivity">

    <com.example.bruno.qrcodereader.CameraSourcePreview
        android:id="@+id/preview"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.example.bruno.qrcodereader.GraphicOverlay
            android:id="@+id/graphicOverlay"
            android:layout_height="match_parent"
            android:layout_width="match_parent"/>
    </com.example.bruno.qrcodereader.CameraSourcePreview>
</RelativeLayout>
